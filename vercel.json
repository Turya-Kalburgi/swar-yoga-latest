{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_BASE_URL": "https://swar-yoga-dec.onrender.com"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://swar-yoga-dec.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
