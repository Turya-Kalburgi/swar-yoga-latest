{
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "cleanUrls": false,
  "trailingSlash": false,
  "framework": "vite",
  "protectionBypass": {
    "revalidateSecretString": "false"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
